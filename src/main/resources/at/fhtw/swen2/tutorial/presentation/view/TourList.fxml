<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableView?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="at.fhtw.swen2.tutorial.presentation.view.TourListView"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Button fx:id="saveEditedTourButton" AnchorPane.rightAnchor="100.0" AnchorPane.topAnchor="10.0" onAction="#saveEditedTourButtonAction" text="update" />
        <Button fx:id="editTourButton" AnchorPane.rightAnchor="100.0" AnchorPane.topAnchor="10.0" onAction="#editButtonAction" text="edit" />
        <Button fx:id="deleteTourButton" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0" onAction="#deleteButtonAction" text="delete" />
        <StackPane fx:id="dataContainer"
                   AnchorPane.leftAnchor="7.0"
                   AnchorPane.rightAnchor="7.0"
                   AnchorPane.topAnchor="50.0"
                   AnchorPane.bottomAnchor="7.0">
        </StackPane>
    </children>

</AnchorPane>
